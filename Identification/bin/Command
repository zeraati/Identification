update [Markaz].dbo.[Themp_Tahzib_Copy_۱۳۹۵۲۲۶] set dbo.[Themp_Tahzib_Copy_۱۳۹۵۲۲۶].[Person.Person.Id] = [Person].dbo.[Person].[Id]FROM [Person].dbo.[Person] INNER JOIN [Markaz].dbo.[Themp_Tahzib_Copy_۱۳۹۵۲۲۶] ON [Person].dbo.A2Extra(dbo.[Themp_Tahzib_Copy_۱۳۹۵۲۲۶].Name,[Person].dbo.[Person].Name)>=95 AND dbo.[Themp_Tahzib_Copy_۱۳۹۵۲۲۶].Name is not null AND [Person].dbo.A2Extra(dbo.[Themp_Tahzib_Copy_۱۳۹۵۲۲۶].Family,[Person].dbo.[Person].Family)>=95 AND dbo.[Themp_Tahzib_Copy_۱۳۹۵۲۲۶].Family is not null AND dbo.[Themp_Tahzib_Copy_۱۳۹۵۲۲۶].Father=[Person].dbo.[Person].Father and dbo.[Themp_Tahzib_Copy_۱۳۹۵۲۲۶].Father is not null  AND dbo.[Themp_Tahzib_Copy_۱۳۹۵۲۲۶].CodeMelli=[Person].dbo.[Person].CodeMelli AND dbo.[Themp_Tahzib_Copy_۱۳۹۵۲۲۶].CodeMelli is not null  WHERE dbo.[Themp_Tahzib_Copy_۱۳۹۵۲۲۶].[Person.Person.Id] IS NULL 